class Solution {
public:
    long long zeroFilledSubarray(vector<int>& nums) {
        
        long long ans = 0;
        long long s = 0;

        for(auto i : nums){

            if(i == 0){
                s++;
                ans += s;
            }else{
                s = 0;
            }
        }

        return ans;
    }
};
